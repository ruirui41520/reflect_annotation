<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.testcoroutine">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:name=".CustomApplication"
        android:allowBackup="true"
        android:excludeFromRecents="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppThemeNew"
        android:usesCleartextTraffic="true">
        <activity android:name=".TestActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity栈.StandardActivity"
            android:launchMode="standard"></activity>
        <activity
            android:name=".Activity栈.SingleTopActivity"
            android:launchMode="singleTop"></activity>
        <activity
            android:name=".Activity栈.SingleTaskActivity"
            android:launchMode="singleTask"
            android:taskAffinity="com.android.singleTask"></activity>
        <activity
            android:name=".Activity栈.SingleInstanceActivity"
            android:launchMode="singleInstance"></activity>
        <activity
            android:name=".Recyclerview使用.CoinsActivity"></activity>
        <activity
            android:name=".AIDL跨进程.ClientAIDLActivity"></activity>
        <activity
            android:name=".LoaderManager使用.LoaderActivity"></activity>
        <activity
            android:name=".SQlite库.PersonDataBaseActivity"></activity>
        <activity
            android:name=".广播.BroadCastActivity"></activity>
        <activity
            android:name=".Service.ServiceActivity"></activity>

        <activity
            android:name=".拉数据.QuarkActivity"></activity>

        <receiver android:name=".广播.StaticReceiver" android:exported="false">
            <intent-filter>
                <action android:name="com.broadcast.static"/>
            </intent-filter>
        </receiver>
        <service android:name=".Service.CustomService"/>
    </application>

</manifest>